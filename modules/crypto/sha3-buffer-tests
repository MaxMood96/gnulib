bench_sha3_256_LDADD = $(LDADD) @LIB_CRYPTO@
bench_sha3_384_CPPFLAGS = $(AM_CPPFLAGS) -DNDEBUG
bench_sha3_384_LDADD = $(LDADD) @LIB_CRYPTO@
bench_sha3_512_CPPFLAGS = $(AM_CPPFLAGS) -DNDEBUG
bench_sha3_512_LDADD = $(LDADD) @LIB_CRYPTO@
